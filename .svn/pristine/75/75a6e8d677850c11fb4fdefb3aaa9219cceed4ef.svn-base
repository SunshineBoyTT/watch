package com.spring.base.websocket;

/**s
 * 系统常量类
 * 从常量配置文件systemConstant.properties中读取常量<br>
 * 所有常量配置到systemConstant.properties中，变量配置到systemConfig.xml文件中
 */
public class Constant {
	
	/**
	 * 接口反馈相关格式定义
	 */
	public static final String InterfaceResponseCharacterEncoding ;
	public static final String InterfaceResponseSuccessContentType ;
	public static final String InterfaceResponseErrorContentType ;
	public static final Integer InterfaceResponseCodeSuccess ;
	public static final Integer InterfaceResponseCodeNullresult ;
	public static final Integer InterfaceResponseCodeError ;
	
	public static final String InterfaceResponseSuccessContentTypeXml ;
	public static final String InterfaceResponseSuccessContentTypeHtml ;
	
	public static final int CacheTime = 30;

	public static final String QR_PREX = "http://10.10.130.223/d/";
	
	//常量配置文件类

	static {
	
		//接口参数传入错误的响应状态码定义
		InterfaceResponseCharacterEncoding = "UTF-8";
		InterfaceResponseSuccessContentType = "application/json;charset=UTF-8";
		InterfaceResponseErrorContentType = "application/text;charset=UTF-8";
		InterfaceResponseCodeSuccess = 200;
		InterfaceResponseCodeNullresult = 300;
		InterfaceResponseCodeError = 400;
		InterfaceResponseSuccessContentTypeXml = "text/xml";
		InterfaceResponseSuccessContentTypeHtml ="text/html";
	}
}
